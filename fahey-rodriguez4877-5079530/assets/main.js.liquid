$(function() {
var $header = $('#main-header');
  $(window).scroll(function() {
    if(!$('html').hasClass('mmenu-opened')) {
      $header.toggleClass('fixed', $(window).scrollTop() > 0);
      }
    });
      $('#SideNavMenu').mmenu({
        position: "{{settings.fw_mmenu_pos}}",
        configuration: {
      hardwareAcceleration: false
    },
    searchfield: false
    });

     $('.search a').click( function() {
        $('input#search-field').focus();    
        });                           
   // $('.fancybox').fancybox();
 
 

/// Collection slider
    jQuery.fn.reverse = [].reverse; //Genius deserves credit: http://stackoverflow.com/questions/1394020/jquery-each-backwards
    
    $('.collection-slider').each(function(){
        var $arrowCont = $(this).find('.arrows');
            $('<a class="previous" href="#">&larr;</a>').hide().prependTo($arrowCont).fadeIn().css('display', 'inline');
            $('<a class="next" href="#">&rarr;</a>').hide().appendTo($arrowCont).fadeIn().css('display', 'inline');
        
    });


        var $collectionSlider = $('.collection-slider');

        
        // Calculate Height


    $collectionSlider.on('click', '.previous, .next', function(){
        //Find items visible in first row
        var nextShow = $(this).hasClass('next');
        var $productItems = $(this).closest('.collection-slider').find('.product-item');
        var $container = $productItems.parent();
        var itemsToMove = [];
        var runningOffset = 0;

        if(nextShow) {
            //Next
            $productItems.each(function(index){
                if(index == 0) {
                    runningOffset = $(this).offset().top;
                    itemsToMove.push($(this));
                } else {
                    if($(this).offset().top == runningOffset) {
                        itemsToMove.push($(this));
                    } else {
                        return false; //Break loop
                    }
                }
            });
        } else {
            //Previous
            var contWidth = $container.width();
            $productItems.reverse().each(function(){
                runningOffset += $(this).outerWidth();
                if(runningOffset <= contWidth) {
                    itemsToMove.push($(this));
                } else {
                    return false; //Break loop
                }
            });
        }
        //Fade out
        $productItems.stop().animate({opacity: 0}, 500, function(){
            //Rearrange
            $.each(itemsToMove, function(index, item){
                if(nextShow) {
                    $container.append(item);
                } else {
                    $container.prepend(item);
                }
            });
            //Fade in
            $productItems.stop().animate({opacity: 1}, 500);
        });
        return false;
    });


});

